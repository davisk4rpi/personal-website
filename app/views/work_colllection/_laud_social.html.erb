<div class="panel-group" id="laud-socialaccordion" role="tablist" aria-multiselectable="true">
  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="laud-socialheadingOne">
      <h4 class="panel-title">
        <a role="button" data-toggle="collapse" data-parent="#laud-socialaccordion"
            href="#laud-socialcollapseOne" aria-expanded="true" aria-controls="laud-socialcollapseOne">
          Background
        </a>
      </h4>
    </div>
    <div id="laud-socialcollapseOne" class="panel-collapse collapse in"
          role="tabpanel" aria-labelledby="laud-socialheadingOne">
      <div class="panel-body">
        <%= render "work_colllection/laud-social/background_body" %>
      </div>
    </div>
  </div>
  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="laud-socialheadingTwo">
      <h4 class="panel-title">
        <a class="collapsed" role="button" data-toggle="collapse"
            data-parent="#laud-socialaccordion" href="#laud-socialcollapseTwo" aria-expanded="false"
            aria-controls="laud-socialcollapseTwo">
          Meeting With Stakeholders
        </a>
      </h4>
    </div>
    <div id="laud-socialcollapseTwo" class="panel-collapse collapse"
          role="tabpanel" aria-labelledby="laud-socialheadingTwo">
      <div class="panel-body">
        <%= render "work_colllection/laud-social/before_body" %>
      </div>
    </div>
  </div>
  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="laud-socialheadingThree">
      <h4 class="panel-title">
        <a class="collapsed" role="button" data-toggle="collapse"
            data-parent="#laud-socialaccordion" href="#laud-socialcollapseThree"
            aria-expanded="false" aria-controls="laud-socialcollapseThree">
          Designing the Tests
        </a>
      </h4>
    </div>
    <div id="laud-socialcollapseThree" class="panel-collapse collapse"
          role="tabpanel" aria-labelledby="laud-socialheadingThree">
      <div class="panel-body">
        <%= render "work_colllection/laud-social/research_body" %>
      </div>
    </div>
  </div>
  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="laud-socialheadingFour">
      <h4 class="panel-title">
        <a class="collapsed" role="button" data-toggle="collapse"
            data-parent="#laud-socialaccordion" href="#laud-socialcollapseFour" aria-expanded="false"
            aria-controls="laud-socialcollapseFour">
          Performing the Tests
        </a>
      </h4>
    </div>
    <div id="laud-socialcollapseFour" class="panel-collapse collapse"
          role="tabpanel" aria-labelledby="laud-socialheadingFour">
      <div class="panel-body">
        <%= render "work_colllection/laud-social/personas_body" %>
      </div>
    </div>
  </div>
  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="laud-socialheadingFive">
      <h4 class="panel-title">
        <a class="collapsed" role="button" data-toggle="collapse"
            data-parent="#laud-socialaccordion" href="#laud-socialcollapseFive" aria-expanded="false"
            aria-controls="laud-socialcollapseFive">
          Analyzing the Results
        </a>
      </h4>
    </div>
    <div id="laud-socialcollapseFive" class="panel-collapse collapse"
          role="tabpanel" aria-labelledby="laud-socialheadingFive">
      <div class="panel-body">
        <%= render "work_colllection/laud-social/paper_body" %>
      </div>
    </div>
  </div>
  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="laud-socialheadingSix">
      <h4 class="panel-title">
        <a class="collapsed" role="button" data-toggle="collapse"
            data-parent="#laud-socialaccordion" href="#laud-socialcollapseSix" aria-expanded="false"
            aria-controls="laud-socialcollapseSix">
          Results
        </a>
      </h4>
    </div>
    <div id="laud-socialcollapseSix" class="panel-collapse collapse"
          role="tabpanel" aria-labelledby="laud-socialheadingSix">
      <div class="panel-body">
        <%= render "work_colllection/laud-social/initial_body" %>
      </div>
    </div>
  </div>
</div>
